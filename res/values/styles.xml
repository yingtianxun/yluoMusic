<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <!-- begin slide_menu Laozuodong -->
    <style name="slide_menu_item_rl">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:clickable">true</item>
        <item name="android:paddingLeft">30dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:background">@drawable/item_slide_menu_bk</item>
    </style>

    <style name="slide_menu_item_rl_yluotx">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:drawablePadding">20dp</item>
        <item name="android:textColor">#ccffffff</item>
        <item name="android:textSize">16sp</item>
        <item name="com.yluo.yluomusic:drawable_height">23dp</item>
        <item name="com.yluo.yluomusic:drawable_width">23dp</item>
        <item name="com.yluo.yluomusic:use_yluo">true</item>
    </style>

    <style name="slide_menu_item_right_tv">
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:textColor">#88ffff</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:visibility">invisible</item>
    </style>

    <style name="slide_menu_item_right_checkbox">
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:padding">0dp</item>
        <item name="android:button">@android:color/transparent</item>
    </style>

    <style name="slide_menu_setting_back">
        <item name="android:drawablePadding">10dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:clickable">true</item>
        <item name="android:textColor">#dddddd</item>
        <item name="com.yluo.yluomusic:drawable_height">23dp</item>
        <item name="com.yluo.yluomusic:drawable_width">23dp</item>
        <item name="com.yluo.yluomusic:use_yluo">true</item>
    </style>
    <!-- end slide_menu Laozuodong -->


    <!-- begin mainContent_listen Laozuodong -->
    <style name="main_content_sing">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableLeft">@drawable/main_content_look_tv_liveshow</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textSize">20dp</item>
    </style>

    <!-- begin mainContent_look Laozuodong -->

    <style name="main_content_listen_live">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15dp</item>
        <item name="android:textColor">#ff060606</item>
        <item name="com.yluo.yluomusic:create_presseddawable">true</item>
        <item name="com.yluo.yluomusic:drawable_height">25dp</item>
        <item name="com.yluo.yluomusic:drawable_width">25dp</item>
        <item name="com.yluo.yluomusic:pressed_textcolor">#64959595</item>
        <item name="com.yluo.yluomusic:use_yluo">true</item>
    </style>

    <style name="main_content_listen_ll">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="main_content_listen_yluo_textview">
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ffffff</item>
        
        <item name="com.yluo.yluomusic:drawable_height">50dp</item>
        <item name="com.yluo.yluomusic:drawable_width">50dp</item>
        <item name="com.yluo.yluomusic:pressed_textcolor">#ff248BD5</item>
        <item name="com.yluo.yluomusic:use_yluo">true</item>
        <item name="com.yluo.yluomusic:create_drawable">false</item>
        
    </style>

    <style name="main_content_listen_yluotextview">
        <item name="android:drawablePadding">5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#ff959595</item>
        <item name="com.yluo.yluomusic:create_presseddawable">true</item>
        <item name="com.yluo.yluomusic:drawable_height">21dp</item>
        <item name="com.yluo.yluomusic:drawable_width">21dp</item>
        <item name="com.yluo.yluomusic:pressed_textcolor">#64959595</item>
        <item name="com.yluo.yluomusic:use_yluo">true</item>
    </style>

    <style name="main_content_listen_mainbutton_textview">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ff2C2C2C</item>
        <item name="android:drawablePadding">15dp</item>
        <item name="com.yluo.yluomusic:drawable_height">50dp</item>
        <item name="com.yluo.yluomusic:drawable_width">50dp</item>
        <item name="com.yluo.yluomusic:pressed_textcolor">#ff248BD5</item>
        <item name="com.yluo.yluomusic:use_yluo">true</item>
        <item name="com.yluo.yluomusic:create_drawable">false</item>
    </style>

    <style name="main_content_look_live">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#55000000</item>
    </style>

    <style name="main_content_live_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:background">#55000000</item>
    </style>
    
       <style name="play_song_select_type_tx">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">1dp</item>
        <item name="android:paddingLeft">4dp</item>
        <item name="android:paddingRight">4dp</item>
        <item name="android:paddingTop">3dp</item>
        <item name="android:paddingBottom">3dp</item>
        <item name="android:layout_marginLeft">6dp</item>
        <item name="android:layout_marginRight">6dp</item>
        <item name="android:textColor">#ffE6EBED</item>
        <item name="android:textSize">12dp</item>
        <item name="android:background">@drawable/playing_song_tag_bk</item>
        <item name="com.yluo.yluomusic:create_drawable">false</item>
        <item name="com.yluo.yluomusic:create_presseddawable">false</item>
        <item name="com.yluo.yluomusic:drawable_height">3dp</item>
        <item name="com.yluo.yluomusic:drawable_width">8dp</item>
        <item name="com.yluo.yluomusic:use_yluo">true</item>
    </style>
    <!-- end mainContent_look Laozuodong -->

</resources>